   } else {\r\n        this.id = JSON.parse(localStorage.getItem(\"Search\")).Ranking.nv[this.obj.id]._id;\r\n        this.axios\r\n          .get(\"http://novel.kele8.cn/rank/\" + this.id)\r\n          .then(response => {\r\n            window.console.log(response);\r\n            this.list = response.data.ranking.books.slice(0, 4);\r\n          });\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.tabdata();\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.shabi {\r\n  width: 100%;\r\n  background-color: #fff;\r\n  padding: 15px 0;\r\n  margin-bottom: 15px;\r\n  .recommend-box {\r\n    width: 100%;\r\n    .title {\r\n      text-align: left;\r\n      border-left: 3px solid #b93321;\r\n      padding-left: 10px;\r\n      font-weight: bolder;\r\n      font-size: 14px;\r\n    }\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}